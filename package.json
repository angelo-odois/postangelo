{
  "name": "revuu",
  "version": "1.0.0",
  "private": true,
  "description": "Revuu - Portfolio platform for professionals",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:backend": "pnpm --filter @revuu/backend dev",
    "dev:frontend": "pnpm --filter @revuu/frontend dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "seed": "pnpm --filter @revuu/backend seed"
  },
  "keywords": ["portfolio", "cms", "block-editor"],
  "author": "Angelo",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
